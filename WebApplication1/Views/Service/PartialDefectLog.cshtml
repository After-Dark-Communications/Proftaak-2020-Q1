<div class="R-Defectlog">
    <div>
        <table style="width: 100%;">
            <tr>
                <td class="R-content R-content-status"></td><!--Status-->
                <td class="R-content R-content-wide"></td><!--Datum-->
            </tr>
            @for (int i = 0; i < 15; i++)
            {
                <tr>
                    <td class="R-content R-content-status"></td><!--Status-->
                    <td class="R-content R-content-wide"></td><!--Datum-->
                </tr>
            }
        </table>
    </div>
    <div>
        <input type="button" class="Closebutton" value="Defectlog Sluiten" button data-dismiss="modal"/>
    </div>
</div>

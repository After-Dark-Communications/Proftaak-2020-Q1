<div class="test1">
    <div class="status-left">
        <div class="status-info">
            <h4 class="status-title">Tram @ViewBag.Tramnumber</h4>
            <h4 class="status-title">Spoornummer: 38</h4>
            <h4 class="status-title">Soort: @ViewBag.Type</h4>
        </div>
        <div class="status-info">
            <p class="status-title">Laatste Schoonmaakbeurt</p>
            <p>Klein: 0 dagen geleden</p>
            <p>Groot: 35 dagen geleden</p>
        </div>
        <div class="status-info">
            <p class="status-title">Laatste Servicebeurt</p>
            <p>Klein: 0 dagen geleden</p>
            <p>Groot: 35 dagen geleden</p>
        </div>
        <div id="getcount" class="status-inline">
            <div>Status:</div>
            <div class="status-defect">Reparatie</div>
            <div class="status-cleaning">Schoonmaak</div>
        </div>
    </div>
    <div class="status-right">
        <input type="button" class="status-buttom" value="Verplaats Tram naar..." onclick="MoveTram(@ViewBag.Tramnumber, 38)"/>
        <input type="button" class="status-buttom" value="Verwijder Tram"/>
        <input type="button" class="status-buttom" value="Schoonmaak Aanmelden"/>
        <input type="button" class="status-buttom" value="Stuur naar Reperatie"/>
    </div>
</div>
@* @ViewBag.Tramnumber = tramData.TramNumber; *@
